{
  "kind": "collectionType",
  "collectionName": "chat_rooms",
  "info": {
    "name": "chat_room",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "searchable": true
  },
  "pluginOptions": {},
  "attributes": {
    "deleted_at": {
      "type": "datetime"
    },
    "taxiFare": {
      "type": "integer"
    },
    "distance": {
      "type": "decimal"
    },
    "start_route": {
      "type": "dynamiczone",
      "searchable": true,
      "components": [
        "route.route"
      ]
    },
    "end_route": {
      "type": "dynamiczone",
      "searchable": true,
      "components": [
        "route.route"
      ]
    },
    "departureTime": {
      "type": "datetime"
    },
    "person_limit": {
      "type": "integer"
    },
    "start_at": {
      "type": "datetime"
    },
    "disable_at": {
      "type": "datetime"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "W",
        "M",
        "None"
      ]
    },
    "creator_id": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "title": {
      "type": "string",
      "searchable": true
    },
    "path": {
      "type": "json"
    },
    "duration": {
      "type": "decimal"
    }
  }
}
